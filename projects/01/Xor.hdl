// File name: projects/01/Xor.hdl

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not		(in=a,	out=d);	//(not)a
	And		(a=d,	b=b,   out=e); //(not)aVb
	Not		(in=b,	out=f);			//(not)b
	And		(a=a,	b=f,   out=g);	//(not)bVa
	Or		(a=e,	b=g,	out=out); //(not)aVb+(not)bVa
}